{"code":"import { __decorate, __metadata } from \"tslib\";\r\nimport { Injectable } from '@angular/core';\r\nimport { HttpParams, HttpClient, HttpErrorResponse } from '@angular/common/http';\r\nimport { throwError as observableThrowError, BehaviorSubject } from 'rxjs';\r\nimport { catchError, switchMap } from 'rxjs/operators';\r\nlet TokenInterceptorService = class TokenInterceptorService {\r\n    constructor(http) {\r\n        this.http = http;\r\n        this.tokenSubject = new BehaviorSubject(null);\r\n    }\r\n    intercept(req, next) {\r\n        const token = localStorage.getItem('Logintoken');\r\n        const modifiedReq = req.clone({\r\n            headers: req.headers.set('Authorization', `Bearer ${token}`),\r\n        });\r\n        //return next.handle(modifiedReq).pipe();\r\n        return next.handle(modifiedReq).pipe(catchError(error => {\r\n            if (error instanceof HttpErrorResponse) {\r\n                switch (error.status) {\r\n                    case 401:\r\n                        let refreshToken = localStorage.getItem('Refreshtoken');\r\n                        return this.refreshtoken(refreshToken).pipe(switchMap((response) => {\r\n                            if (response) {\r\n                                this.token = response.access_token;\r\n                                localStorage.setItem('Logintoken', this.token);\r\n                                localStorage.setItem('Refreshtoken', response.refresh_token);\r\n                                let newToken = this.token;\r\n                                this.tokenSubject.next(newToken);\r\n                                return next.handle(this.addToken(req, newToken));\r\n                            }\r\n                        }));\r\n                    default:\r\n                        return observableThrowError(error);\r\n                }\r\n            }\r\n            else {\r\n                return observableThrowError(error);\r\n            }\r\n        }));\r\n    }\r\n    refreshtoken(refreshToken) {\r\n        const payload = new HttpParams()\r\n            .set('refresh_token', refreshToken)\r\n            .set('grant_type', 'refresh_token')\r\n            .set('client_id', 'MagnarClientId')\r\n            .set('client_secret', 'MagnarClientIdKey');\r\n        return this.http\r\n            .post('http://localhost:1478/connect/token', payload)\r\n            .pipe();\r\n    }\r\n    addToken(req, token) {\r\n        return req.clone({ setHeaders: { Authorization: 'Bearer ' + token } });\r\n    }\r\n};\r\nTokenInterceptorService = __decorate([\r\n    Injectable(),\r\n    __metadata(\"design:paramtypes\", [HttpClient])\r\n], TokenInterceptorService);\r\nexport { TokenInterceptorService };\r\n//# sourceMappingURL=Token-interceptor.js.map","map":{"version":3,"file":"Token-interceptor.js","sourceRoot":"","sources":["../../src/app/service/Token-interceptor.ts"],"names":[],"mappings":";AAAA,OAAO,EAAE,UAAU,EAAE,MAAM,eAAe,CAAC;AAC3C,OAAO,EAAwD,UAAU,EAAE,UAAU,EAAE,iBAAiB,EAAE,MAAM,sBAAsB,CAAC;AACvI,OAAO,EAAE,UAAU,IAAI,oBAAoB,EAAc,eAAe,EAAE,MAAM,MAAM,CAAC;AACvF,OAAO,EAAE,UAAU,EAAE,SAAS,EAAE,MAAM,gBAAgB,CAAC;AAGvD,IAAa,uBAAuB,GAApC,MAAa,uBAAuB;IAKlC,YAAoB,IAAgB;QAAhB,SAAI,GAAJ,IAAI,CAAY;QAF7B,iBAAY,GAA4B,IAAI,eAAe,CAAS,IAAI,CAAC,CAAC;IAEzC,CAAC;IAEzC,SAAS,CAAC,GAAqB,EAAE,IAAiB;QAChD,MAAM,KAAK,GAAG,YAAY,CAAC,OAAO,CAAC,YAAY,CAAC,CAAC;QACjD,MAAM,WAAW,GAAG,GAAG,CAAC,KAAK,CAAC;YAC5B,OAAO,EAAE,GAAG,CAAC,OAAO,CAAC,GAAG,CAAC,eAAe,EAAE,UAAU,KAAK,EAAE,CAAC;SAC7D,CAAC,CAAC;QACH,yCAAyC;QAEzC,OAAO,IAAI,CAAC,MAAM,CAAC,WAAW,CAAC,CAAC,IAAI,CAElC,UAAU,CAAC,KAAK,CAAC,EAAE;YAEjB,IAAI,KAAK,YAAY,iBAAiB,EAAE;gBAEtC,QAA4B,KAAM,CAAC,MAAM,EAAE;oBAGzC,KAAK,GAAG;wBACN,IAAI,YAAY,GAAG,YAAY,CAAC,OAAO,CAAC,cAAc,CAAC,CAAC;wBACxD,OAAO,IAAI,CAAC,YAAY,CAAC,YAAY,CAAC,CAAC,IAAI,CACzC,SAAS,CAAC,CAAC,QAAa,EAAE,EAAE;4BAE1B,IAAI,QAAQ,EAAE;gCAEZ,IAAI,CAAC,KAAK,GAAS,QAAS,CAAC,YAAY,CAAC;gCAC1C,YAAY,CAAC,OAAO,CAAC,YAAY,EAAE,IAAI,CAAC,KAAK,CAAC,CAAC;gCAC/C,YAAY,CAAC,OAAO,CAAC,cAAc,EAAQ,QAAS,CAAC,aAAa,CAAC,CAAC;gCAEpE,IAAI,QAAQ,GAAG,IAAI,CAAC,KAAK,CAAC;gCAC1B,IAAI,CAAC,YAAY,CAAC,IAAI,CAAC,QAAQ,CAAC,CAAC;gCACjC,OAAO,IAAI,CAAC,MAAM,CAAC,IAAI,CAAC,QAAQ,CAAC,GAAG,EAAE,QAAQ,CAAC,CAAC,CAAC;6BAElD;wBACH,CAAC,CAAC,CAEH,CAAC;oBACJ;wBAEE,OAAO,oBAAoB,CAAC,KAAK,CAAC,CAAC;iBAEtC;aAEF;iBAAM;gBAEL,OAAO,oBAAoB,CAAC,KAAK,CAAC,CAAC;aAEpC;QAEH,CAAC,CAAC,CAEH,CAAC;IACJ,CAAC;IAGD,YAAY,CAAC,YAAY;QACvB,MAAM,OAAO,GAAG,IAAI,UAAU,EAAE;aAC7B,GAAG,CAAC,eAAe,EAAE,YAAY,CAAC;aAClC,GAAG,CAAC,YAAY,EAAE,eAAe,CAAC;aAClC,GAAG,CAAC,WAAW,EAAE,gBAAgB,CAAC;aAClC,GAAG,CAAC,eAAe,EAAE,mBAAmB,CAAC,CAAC;QAC7C,OAAO,IAAI,CAAC,IAAI;aACb,IAAI,CAAC,qCAAqC,EAAE,OAAO,CAAC;aACpD,IAAI,EAAE,CAAC;IACZ,CAAC;IAED,QAAQ,CAAC,GAAqB,EAAE,KAAa;QAE3C,OAAO,GAAG,CAAC,KAAK,CAAC,EAAE,UAAU,EAAE,EAAE,aAAa,EAAE,SAAS,GAAG,KAAK,EAAE,EAAE,CAAC,CAAA;IAExE,CAAC;CACF,CAAA;AA5EY,uBAAuB;IADnC,UAAU,EAAE;qCAMe,UAAU;GALzB,uBAAuB,CA4EnC;SA5EY,uBAAuB"}}
