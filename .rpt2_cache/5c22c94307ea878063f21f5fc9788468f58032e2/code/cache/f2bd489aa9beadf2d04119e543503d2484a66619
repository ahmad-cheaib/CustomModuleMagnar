{"code":"import { takeWhile, filter, tap } from \"rxjs/operators\";\r\nimport { isNullOrUndefined } from \"util\";\r\nexport class LeaveBaseComponent {\r\n    constructor(datePipe, _leaveRequestService, _ConfigurationService, _EmployeeLookupService, _WorkflowtemplateService) {\r\n        this.datePipe = datePipe;\r\n        this._leaveRequestService = _leaveRequestService;\r\n        this._ConfigurationService = _ConfigurationService;\r\n        this._EmployeeLookupService = _EmployeeLookupService;\r\n        this._WorkflowtemplateService = _WorkflowtemplateService;\r\n        this.companyId = 1;\r\n        this.organizationId = '1';\r\n        this.isDestroyed = false;\r\n    }\r\n    ngOnDestroy() {\r\n        this.isDestroyed = true;\r\n    }\r\n    getDateFormat() {\r\n        this.calendarDateFormat = this._ConfigurationService.pCalendarDateFormat;\r\n    }\r\n    transformDate(date) {\r\n        return this.datePipe.transform(date, 'yyyy/MM/dd');\r\n    }\r\n    getEmployees() {\r\n        return this._EmployeeLookupService.getAllEmployeesLookups(this.companyId)\r\n            .pipe(takeWhile(() => !this.isDestroyed), filter(response => !isNullOrUndefined(response)), tap(response => this.employees = response));\r\n    }\r\n    getLeaves() {\r\n        return this._leaveRequestService.GetLeaves()\r\n            .pipe(takeWhile(() => !this.isDestroyed), filter(response => !isNullOrUndefined(response)), tap(response => this.leaves = response));\r\n    }\r\n    getWorkflowStatuses() {\r\n        return this._WorkflowtemplateService.getWorkflowStatuses()\r\n            .pipe(takeWhile(() => !this.isDestroyed), filter(response => !isNullOrUndefined(response)), tap(response => this.workflowStatuses = response));\r\n    }\r\n}\r\n//# sourceMappingURL=leave-base.component.js.map","map":{"version":3,"file":"leave-base.component.js","sourceRoot":"","sources":["../../src/app/leave-request/leave-base.component.ts"],"names":[],"mappings":"AAEA,OAAO,EAAE,SAAS,EAAE,MAAM,EAAE,