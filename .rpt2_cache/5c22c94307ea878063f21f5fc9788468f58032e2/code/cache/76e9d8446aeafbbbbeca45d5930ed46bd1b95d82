{"code":"import { __decorate, __metadata } from \"tslib\";\r\nimport { Injectable } from '@angular/core';\r\nimport { HttpClient } from '@angular/common/http';\r\nimport { UtilityService } from './utility.service';\r\nimport { ConfigurationService } from './configuration.service';\r\nimport { catchError } from 'rxjs/operators';\r\nlet ExtendedPropertiesService = class ExtendedPropertiesService {\r\n    constructor(http, utilityService, configurationService) {\r\n        this.http = http;\r\n        this.utilityService = utilityService;\r\n        this.configurationService = configurationService;\r\n        this.urlAPI = this.configurationService.baseUrl + '/api/ExtendedProperties';\r\n    }\r\n    getAllByCompany(id, top, skip, orderby, direction) {\r\n        return this.http\r\n            .get(this.urlAPI + `/OdataGet?$orderby=${orderby} ${direction}&$filter=companyId eq ${id}&$top=${top}&$skip=${skip}`)\r\n            .pipe(catchError(this.utilityService.handleErrorPromise));\r\n    }\r\n    get(id) {\r\n        return this.http\r\n            .get(this.urlAPI + `/Get?id=${id}`)\r\n            .pipe(catchError(this.utilityService.handleErrorPromise));\r\n    }\r\n    getModel() {\r\n        return this.http\r\n            .get(this.urlAPI + `/GetModel`)\r\n            .pipe(catchError(this.utilityService.handleErrorPromise));\r\n    }\r\n    post(data) {\r\n        return this.http.post(this.urlAPI + '/Post', JSON.stringify(data)).pipe(catchError(this.utilityService.handleErrorPromise));\r\n    }\r\n    put(data) {\r\n        return this.http.put(this.urlAPI + '/Put', JSON.stringify(data)).pipe(catchError(this.utilityService.handleErrorPromise));\r\n    }\r\n    delete(id) {\r\n        return this.http\r\n            .delete(this.urlAPI + '/delete?id=' + id)\r\n            .pipe(catchError(this.utilityService.handleErrorPromise));\r\n    }\r\n    getSections(containerType) {\r\n        return this.http\r\n            .get(this.urlAPI + `/GetSections?containerType=${containerType}`)\r\n            .pipe(catchError(this.utilityService.handleErrorPromise));\r\n    }\r\n    getByCompany(companyId) {\r\n        return this.http\r\n            .get(this.urlAPI + '/GetByCompany?companyId=' + companyId)\r\n            .pipe(catchError(this.utilityService.handleErrorPromise));\r\n    }\r\n    getBySection(containerId, containerType, entityName, sectionName) {\r\n        return this.http\r\n            .get(this.urlAPI + `/GetBySection?containerId=${containerId}&containerType=${containerType}&entityName=${entityName}&sectionName=${sectionName}`)\r\n            .pipe(catchError(this.utilityService.handleErrorPromise));\r\n    }\r\n    getByContainer(containerId, containerType) {\r\n        return this.http\r\n            .get(this.urlAPI + '/GetByContainer?containerId=' + containerId + \"&containerType=\" + containerType)\r\n            .pipe(catchError(this.utilityService.handleErrorPromise));\r\n    }\r\n};\r\nExtendedPropertiesService = __decorate([\r\n    Injectable({\r\n        providedIn: 'root'\r\n    }),\r\n    __metadata(\"design:paramtypes\", [HttpClient,\r\n        UtilityService,\r\n        ConfigurationService])\r\n], ExtendedPropertiesService);\r\nexport { ExtendedPropertiesService };\r\n//# sourceMappingURL=extendedProperties.service.js.map","map":{"version":3,"file":"extendedProperties.service.js","sourceRoot":"","sources":["../../src/app/service/extendedProperties.service.ts"],"names":[],"mappings":";AAAA,OAAO,EAAE,UAAU,EAAE,MAAM,eAAe,CAAC;AAC3C,OAAO,EAAE,UAAU,EAAE,MAAM,sBAAsB,CAAC;AAClD,OAAO,EAAE,cAAc,EAAE,MAAM,mBAAmB,CAAC;AACnD,OAAO,EAAE,oBAAoB,EAAE,MAAM,yBAAyB,CAAC;AAC/D,OAAO,EAAE,UAAU,EAAE,MAAM,gBAAgB,CAAC;AAK5C,IAAa,yBAAyB,GAAtC,MAAa,yBAAyB;IAIrC,YACS,IAAgB,EAChB,cAA8B,EAC9B,oBAA0C;QAF1C,SAAI,GAAJ,IAAI,CAAY;QAChB,mBAAc,GAAd,cAAc,CAAgB;QAC9B,yBAAoB,GAApB,oBAAoB,CAAsB;QAClD,IAAI,CAAC,MAAM,GAAG,IAAI,CAAC,oBAAoB,CAAC,OAAO,GAAG,yBAAyB,CAAC;IAC7E,CAAC;IAED,eAAe,CAAC,EAAE,EAAE,GAAG,EAAE,IAAI,EAAE,OAAO,EAAE,SAAS;QAChD,O