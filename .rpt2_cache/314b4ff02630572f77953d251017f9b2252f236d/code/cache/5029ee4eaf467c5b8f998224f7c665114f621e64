{"code":"import { __decorate, __metadata } from \"tslib\";\r\nimport { Injectable } from '@angular/core';\r\nimport { TranslateService } from '@ngx-translate/core';\r\nimport { ToastrService } from 'ngx-toastr';\r\nlet NotificationService = class NotificationService {\r\n    constructor(toastr, translateService) {\r\n        this.toastr = toastr;\r\n        this.translateService = translateService;\r\n        this.success = (body, title = 'Operation successful') => {\r\n            this.toastr.success(body, title);\r\n        };\r\n        this.error = (body, title = 'An error occured') => {\r\n            this.toastr.error(body === \"\" ? title : body, title);\r\n        };\r\n        this.warning = (body) => {\r\n            this.toastr.warning(body);\r\n        };\r\n        this.info = (messageKey, titleKey = 'notifications.NoticeTitle') => {\r\n            this.toastr.info(messageKey, titleKey);\r\n        };\r\n        this.completedSuccessfully = (messageKey = 'notifications.SuccessMessage', titleKey = 'notifications.SuccessTitle') => {\r\n            this.success(messageKey, titleKey);\r\n        };\r\n        this.failure = (messageKey = 'notifications.ErrorMessage', titleKey = 'notifications.ErrorTitle') => {\r\n            this.error(messageKey, titleKey);\r\n        };\r\n    }\r\n};\r\nNotificationService = __decorate([\r\n    Injectable(),\r\n    __metadata(\"design:paramtypes\", [ToastrService, TranslateService])\r\n], NotificationService);\r\nexport { NotificationService };\r\n//# sourceMappingURL=notification.service.js.map","map":{"version":3,"file":"notification.service.js","sourceRoot":"","sources":["../../src/app/service/notification.service.ts"],"names":[],"mappings":";AAAA,OAAO,EAAE,UAAU,EAAE,MAAM,eAAe,CAAC;AAC3C,OAAO,EAAE,gBAAgB,EAAE,MAAM,qBAAqB,CAAC;AACvD,OAAO,EAAE,aAAa,EAAE,MAAM,YAAY,CAAC;AAG3C,IAAa,mBAAmB,GAAhC,MAAa,mBAAmB;IAC/B,YAAoB,MAAqB,EAAU,gBAAkC;QAAjE,WAAM,GAAN,MAAM,CAAe;QAAU,qBAAgB,GAAhB,gBAAgB,CAAkB;QAE9E,YAAO,GAAG,CAAC,IAAY,EAAE,